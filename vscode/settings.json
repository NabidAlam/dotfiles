    "editor.fontFamily": "SauceCodePro NF, Menlo, Monaco, 'Courier New', monospace",
    "editor.codeLens": false,
    "csharp.suppressDotnetRestoreNotification": true,
    "typescript.check.tscVersion": false,
    "workbench.iconTheme": "vs-seti",
    "vim.leader": " ",
